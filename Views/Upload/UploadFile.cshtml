@using aspbasic.service;

@{

    var result = (ImportResult)@ViewData["UploadResult"];
}


<h1>Upload Result</h1>

   @if(result.isError){
        <h2 class="text-danger">An error occurred while processing your request.</h2>
        <h2 class="text-danger">@result.errorText </h2>
   }else {
        <h2>Imported @result.imported </h2>
        <h2>Skipped @result.skipped </h2>
            
   }
